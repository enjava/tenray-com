		<script>
			var J = jQuery;
			var needLoadEmp = ''
			J(function() {
				var selectContent = J('ul.conpany-ul');

				J('body').on('click', function() {
					if(!selectContent.is(":hidden")) {
						selectContent.hide();
					}
				});

				J('#select-company').click(function(e) {
					_stopPropagation(e);
					if(selectContent.is(':hidden')) {
						selectContent.show();
					} else {
						selectContent.hide();
					}
				});

				J('body').on('click', 'ul.conpany-ul li', function(e) {
					_stopPropagation(e);
					var jli = J(this);
					var cid = jli.data('cid');
					var currentCid = J('#select-company .current-company-name').data('cid');
					if(cid == currentCid) {
						selectContent.hide();
						return;
					}
					J.post('/forward/user/doSelect4hr.htm', {
						cid: cid
					}, function(data) {
						if(data.result == 0) {
							window.location.href = "/employee/route.htm";
						} else {
							J.tips.error(data.message);
						}
					})
				});

				function _stopPropagation(e) {
					if(e && e.stopPropagation) {
						e.stopPropagation();
					} else {
						window.event.cancelBubble = true;
					}
				}

			})
		</script>
